package q11042;
class SelectionSortingSmallestElement {	
	public void selectionSortSmallestEle(int[] array) {		
		int j,temp,n=array.length;		
		for(int i=0; i<n; i++) {			
			for(j=i+1; j<n; j++) {				
				if(array[i]>array[j]) {					
					temp=array[i];					
					array[i]=array[j];					
					array[j]=temp;				
				}			
				
			}		
			
		}		
		for(int i=0; i<n; i++) {			
			System.out.println(array[i]);		
		}	
	}
}
